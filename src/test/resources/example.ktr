    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.sql.Timestamp;
import java.time.LocalDateTime;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
	Object[] row = getRow();

	if (row == null) {
    	setOutputDone();
   		return false;
  	}

  	row = createOutputRow(row, data.outputRowMeta.size());

	final Timestamp openDate = get(Fields.In, "open_date").getTimestamp(row);
	final LocalDateTime openDateTime = openDate.toLocalDateTime();	
 	final Integer year = openDateTime.getYear();
 	final Integer month = openDateTime.getMonthValue();
	final Integer day = openDateTime.getDayOfMonth();
	final Timestamp newOpenDate = new Timestamp(year,month,day,0,0,0,0);
	get(Fields.Out, "open_date").setValue(row, newOpenDate);
	putRow(data.outputRowMeta, row);
	return true;
}
</class_source>
