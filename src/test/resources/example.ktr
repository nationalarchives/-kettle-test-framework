    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.sql.Timestamp;
import java.time.LocalDateTime;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
	if (first) {
    	first = false;
  	}

  	Object[] row = getRow();
	boolean rowInError = false;
  	String errMsg = "";
  	int errCnt = 0;

  	if (row == null) {
    	setOutputDone();
    	return false;
  	}
  	String lastDateString = "";
 	String openDateString = "";
  	row = createOutputRow(row, data.outputRowMeta.size());
	final Long lastDate = get(Fields.In, "last_date").getLong(row);
	final Timestamp openDate = get(Fields.In, "open_date").getTimestamp(row);
	if(lastDate != null) {
		lastDateString = lastDate.toString();
	}
  	if(openDate != null) {
		final LocalDateTime openDateTime = openDate.toLocalDateTime();	
 		final Integer year = openDateTime.getYear();
 		final Integer month = openDateTime.getMonthValue();
 		final Integer day = openDateTime.getDayOfMonth();
		openDateString = year.toString() + ((month &lt; 10)? "0" + month.toString() : month.toString()) +
			((day &lt; 10)? "0" + day.toString() : day.toString());

	} else if(lastDateString.length() == 8) {
		openDateString = lastDateString;
  	} else {
		rowInError = true;
    	errMsg = "Unable to get record opening date from last_date:[" + lastDate +"] or open_date:[" + openDate + "]";
	  	errCnt = errCnt + 1;	
  	}
	
	get(Fields.Out, "open_date_string").setValue(row, openDateString);

 	if(!rowInError){
    	putRow(data.outputRowMeta, row);
  	} else {
    	putError(data.outputRowMeta, row, errCnt, errMsg, "open_date_string","ERROR_01");
  	}
	
	return true;
}
</class_source>
      </definition>
